#!/usr/bin/env bash
# Init script to manage the execution of manage_my_process
case $1 in
  start)
    ./manage_my_process.sh start
    ;;
    
  stop)
    ./manage_my_process.sh stop
    ;;
    
  restart)
    ./manage_my_process.sh restart
    ;;
    
  *)
    echo "Usage: $0 {start|stop|restart}"
    exit 1
    ;;
esac

